{
  "name": "smartdocs-server",
  "version": "1.0.0",
  "description": "Backend server for SmartDocs application",
  "main": "index.ts",
  "scripts": {
    "dev": "nodemon --exec ts-node index.ts",
    "build": "tsc && npx prisma generate",
    "start": "npx prisma migrate deploy && node dist/index.js",
